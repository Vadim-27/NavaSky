    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParents": "avoid",
    "proseWrap": "always",
    "plugins": ["prettier-plugin-tailwindcss"]
}
